an class=\"p\"\u003e,\u003c/span\u003e \u003cspan class=\"n\"\u003eprotocol\u003c/span\u003e\u003cspan class=\"p\"\u003e,\u003c/span\u003e \u003cspan class=\"n\"\u003erequesttimeout\u003c/span\u003e\n\u003c/span\u003e\u003c/span\u003e\u003cspan class=\"line\"\u003e\u003cspan class=\"cl\"\u003e\u003cspan class=\"n\"\u003eName\u003c/span\u003e                             \u003cspan class=\"n\"\u003ePort\u003c/span\u003e \u003cspan class=\"n\"\u003eProtocol\u003c/span\u003e \u003cspan class=\"n\"\u003eRequestTimeout\u003c/span\u003e\n\u003c/span\u003e\u003c/span\u003e\u003cspan class=\"line\"\u003e\u003cspan class=\"cl\"\u003e\u003cspan class=\"p\"\u003e----\u003c/span\u003e                             \u003cspan class=\"p\"\u003e----\u003c/span\u003e \u003cspan class=\"p\"\u003e--------\u003c/span\u003e \u003cspan class=\"p\"\u003e-------------\u003c/span\u003e\u003cspan class=\"n\"\u003e-http_t-www\u003c/span\u003e\u003cspan class=\"p\"\u003e.\u003c/span\u003e\u003cspan class=\"py\"\u003emysite\u003c/span\u003e\u003cspan class=\"p\"\u003e.\u003c/span\u003e\u003cspan class=\"py\"\u003ecom\u003c/span\u003e              \u003cspan class=\"mf\"\u003e80\u003c/span\u003e \u003cspan class=\"n\"\u003eHttp\u003c/span\u003e                 \u003cspan class=\"n\"\u003e20https_t-www\u003c/span\u003e\u003cspan class=\"p\"\u003e.\u003c/span\u003e\u003cspan class=\"py\"\u003emysite\u003c/span\u003e\u003cspan class=\"p\"\u003e.\u003c/span\u003e\u003cspan class=\"py\"\u003ecom\u003c/span\u003e            \u003cspan class=\"mf\"\u003e443\u003c/span\u003e \u003cspan class=\"n\"\u003eHttps\u003c/span\u003e                \u003cspan class=\"mf\"\u003e20\u003c/span\u003e\u003c/span\u003e\u003c/span\u003e\u003c/code\u003e\u003c/pre\u003e\n\u003c/div\u003e\n",
